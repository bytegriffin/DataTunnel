<?xml version="1.0" encoding="UTF-8"?>
<!-- 定义task任务集合 -->
<tasks>
    <task>
    	<name>task123</name>
    	<operation>sync</operation>
    	<timer_start></timer_start>
    	<timer_interval></timer_interval>
    	<readers>
    		<reader>
    			<type>redis</type>
    			<address>localhost</address>
    			<value>select kk,vv from page</value>
    		</reader>
    		<reader>
    			<type>222</type>
    			<address>loc3333333alhost</address>
    			<value>3333 333,vv from page</value>
    		</reader>
    	</readers>
  		<writers>
    		<writer>
    			<type>redis</type>
    			<address>localhost</address>
    			<value>insert into topic3(key,value) values ('${kk}','${vv}')</value>
    		</writer>
    	</writers>
    </task>

</tasks>
